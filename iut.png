        if (e.ctrlKey && e.keyCode == 13) {
            var selected_text = orfo.getSelectionText();
            var selected_text_length = selected_text.length;
            if (selected_text_length > 0) {
                if (selected_text_length >= 5 && selected_text_length <= 20) {
                    orfo.getCorrectString(selected_text);
