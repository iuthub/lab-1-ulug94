            con += '<param name="flashvars" value="file=' + this.href + '&autoplay=1" />';
            con += '<embed type="application/x-shockwave-flash"';
            con += '	id="player2"';
            con += '	name="player2"';
            con += '	src="' + '/static/inha/flv_embed/player.swf' + '"';
            con += '	width="400"';
            con += '	height="315"';
            con += '	allowscriptaccess="always"';
            con += '	allowfullscreen="true"';
            con += '	flashvars="file=' + this.href + '"';
            con += '/>';
            con += '</object>';
            this.content = con;
        }
    });

    jQuery('.dropdown-toggle').click(function () {
        if (document.body.clientWidth < 768) {
            jQuery(this).next('ul').toggle();
            return false;
        }
    });

    var all_toggles = jQuery('.newsAndEventsBlock').find('a[href*="#"]');
    all_toggles.each(function () {
        if ($(this).attr('href').length > 3) {
            jQuery($(this).attr('href')).css('display', 'none');
        }
    });
    all_toggles.click(function () {
        if ($(this).attr('href').length > 3) {
            jQuery($(this).attr('href')).toggle();

        }

        return false;
    });
    orfo.init();
});


</script>

